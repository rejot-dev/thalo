{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ptall",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "entry"
          },
          {
            "type": "SYMBOL",
            "name": "_nl"
          }
        ]
      }
    },
    "entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "metadata"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "content"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_nl": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "timestamp",
          "content": {
            "type": "SYMBOL",
            "name": "timestamp"
          }
        },
        {
          "type": "FIELD",
          "name": "directive",
          "content": {
            "type": "SYMBOL",
            "name": "directive"
          }
        },
        {
          "type": "FIELD",
          "name": "entity",
          "content": {
            "type": "SYMBOL",
            "name": "entity"
          }
        },
        {
          "type": "FIELD",
          "name": "title",
          "content": {
            "type": "SYMBOL",
            "name": "title"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "link"
              },
              {
                "type": "SYMBOL",
                "name": "tag"
              }
            ]
          }
        }
      ]
    },
    "metadata": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "key"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "value"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "content": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_nl"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_content_blank"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_content_first"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "markdown_header"
              },
              {
                "type": "SYMBOL",
                "name": "content_line"
              },
              {
                "type": "SYMBOL",
                "name": "_content_blank"
              }
            ]
          }
        }
      ]
    },
    "_content_first": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "markdown_header"
        },
        {
          "type": "SYMBOL",
          "name": "content_line"
        }
      ]
    },
    "_content_blank": {
      "type": "SYMBOL",
      "name": "_nl"
    },
    "markdown_header": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_indent"
          },
          {
            "type": "SYMBOL",
            "name": "_md_hashes"
          },
          {
            "type": "SYMBOL",
            "name": "_md_text"
          },
          {
            "type": "SYMBOL",
            "name": "_eol"
          }
        ]
      }
    },
    "_md_hashes": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "#+"
      }
    },
    "_md_text": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": " [^\\r\\n]+"
      }
    },
    "content_line": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_indent"
          },
          {
            "type": "SYMBOL",
            "name": "_content_text"
          },
          {
            "type": "SYMBOL",
            "name": "_eol"
          }
        ]
      }
    },
    "_content_text": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^#\\r\\n][^\\r\\n]*"
      }
    },
    "_indent": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "  "
      }
    },
    "_eol": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "timestamp": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[12]\\d{3}-[01]\\d-[0-3]\\dT[0-2]\\d:[0-5]\\d"
      }
    },
    "directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "create"
        },
        {
          "type": "STRING",
          "value": "update"
        }
      ]
    },
    "entity": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "lore"
        },
        {
          "type": "STRING",
          "value": "opinion"
        },
        {
          "type": "STRING",
          "value": "reference"
        },
        {
          "type": "STRING",
          "value": "journal"
        }
      ]
    },
    "title": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\"[^\"]*\""
      }
    },
    "link": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\^[A-Za-z0-9\\-_/.]+"
      }
    },
    "tag": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "#[A-Za-z0-9\\-_/.]+"
      }
    },
    "key": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-z][a-zA-Z0-9\\-_]*"
      }
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "link"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_plain_value"
        }
      ]
    },
    "_quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\"[^\"]*\""
      }
    },
    "_plain_value": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n]+"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": " +"
    }
  ],
  "conflicts": [["entry"], ["content"]],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}
