{
  "name": "@rejot-dev/thalo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./native": {
      "development": "./src/parser.native.ts",
      "types": "./dist/parser.native.d.ts",
      "default": "./dist/parser.native.js"
    },
    "./web": {
      "development": "./src/parser.web.ts",
      "types": "./dist/parser.web.d.ts",
      "default": "./dist/parser.web.js"
    },
    "./ast": {
      "development": "./src/ast/index.ts",
      "types": "./dist/ast/index.d.ts",
      "default": "./dist/ast/index.js"
    },
    "./model": {
      "development": "./src/model/index.ts",
      "types": "./dist/model/index.d.ts",
      "default": "./dist/model/index.js"
    },
    "./schema": {
      "development": "./src/schema/index.ts",
      "types": "./dist/schema/index.d.ts",
      "default": "./dist/schema/index.js"
    },
    "./checker": {
      "development": "./src/checker/index.ts",
      "types": "./dist/checker/index.d.ts",
      "default": "./dist/checker/index.js"
    },
    "./services": {
      "development": "./src/services/index.ts",
      "types": "./dist/services/index.d.ts",
      "default": "./dist/services/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "types:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@rejot-dev/tree-sitter-thalo": "workspace:*",
    "tree-sitter": "catalog:",
    "web-tree-sitter": "catalog:"
  },
  "devDependencies": {
    "@rejot-dev/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
